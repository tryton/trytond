        <record model="ir.ui.view" id="module_view_form">
            <field name="model">ir.module.module</field>
            <field name="inherit" ref="ir.module_view_form"/>
            <field name="arch" type="xml">
                <![CDATA[
                    <data>
                        <xpath
                            expr="/form/group/button[@name=&quot;button_install&quot;]"
                            position="replace">
                            <button string="Mark for Installation" name="button_install"
                                type="object"
                                states="{'invisible': Not(Equal(Eval('state'), 'uninstalled')), 'readonly': Not(In(%(res.group_admin)d, Eval('groups', [])))}"/>
                        </xpath>
                        <xpath
                            expr="/form/group/button[@name=&quot;button_install_cancel&quot;]"
                            position="replace">
                            <button string="Cancel Installation" name="button_install_cancel"
                                type="object"
                                states="{'invisible': Not(Equal(Eval('state'), 'to install')), 'readonly': Not(In(%(res.group_admin)d, Eval('groups', [])))}"/>
                        </xpath>
                        <xpath
                            expr="/form/group/button[@name=&quot;button_uninstall&quot;]"
                            position="replace">
                            <button string="Mark for Uninstallation (beta)" name="button_uninstall"
                                type="object"
                                states="{'invisible': Not(Equal(Eval('state'), 'installed')), 'readonly': Bool(True)}"/>
                        </xpath>
                        <xpath
                            expr="/form/group/button[@name=&quot;button_uninstall_cancel&quot;]"
                            position="replace">
                            <button string="Cancel Uninstallation" name="button_uninstall_cancel"
                                type="object"
                                states="{'invisible': Not(Equal(Eval('state'), 'to remove')), 'readonly': Not(In(%(res.group_admin)d, Eval('groups', [])))}"/>
                        </xpath>
                        <xpath
                            expr="/form/group/button[@name=&quot;button_upgrade&quot;]"
                            position="replace">
                            <button string="Mark for Upgrade" name="button_upgrade"
                                type="object"
                                states="{'invisible': Not(Equal(Eval('state'), 'installed')), 'readonly': Not(In(%(res.group_admin)d, Eval('groups', [])))}"/>
                        </xpath>
                        <xpath
                            expr="/form/group/button[@name=&quot;button_upgrade_cancel&quot;]"
                            position="replace">
                            <button string="Cancel Upgrade" name="button_upgrade_cancel"
                                type="object"
                                states="{'invisible': Not(Equal(Eval('state'), 'to upgrade')), 'readonly': Not(In(%(res.group_admin)d, Eval('groups', [])))}"/>
                        </xpath>
                    </data>
                ]]>
            </field>
        </record>
        <record model="res.request.link" id="request_link_cron">
            <field name="name">Scheduler</field>
            <field name="model">ir.cron</field>
        </record>

        <record model="ir.ui.view" id="sequence_type_view_form">
          <field name="model">ir.sequence.type</field>
          <field name="inherit" ref="ir.sequence_type_view_form"/>
          <field name="arch" type="xml">
                <![CDATA[
                    <data>
                        <xpath
                            expr="/form/field[@name=&quot;code&quot;]"
                            position="after">
                          <separator name="groups" colspan="4"/>
                          <field name="groups" colspan="4"/>
                        </xpath>
                    </data>
                ]]>
            </field>
        </record>

        <record model="ir.rule.group" id="rule_group_sequence">
            <field name="model" search="[('model', '=', 'ir.sequence')]"/>
            <field name="global_p" eval="True"/>
            <field name="perm_read" eval="False"/>
        </record>
        <record model="ir.rule" id="rule_sequence">
            <field name="field" search="[('name', '=', 'groups'), ('model.model', '=', 'ir.sequence')]"/>
            <field name="operator">in</field>
            <field name="operand">User/Groups</field>
            <field name="rule_group" ref="rule_group_sequence"/>
        </record>

        <record model="ir.rule.group" id="rule_group_sequence_strict">
            <field name="model" search="[('model', '=', 'ir.sequence.strict')]"/>
            <field name="global_p" eval="True"/>
            <field name="perm_read" eval="False"/>
        </record>
        <record model="ir.rule" id="rule_sequence_strict">
            <field name="field" search="[('name', '=', 'groups'), ('model.model', '=', 'ir.sequence.strict')]"/>
            <field name="operator">in</field>
            <field name="operand">User/Groups</field>
            <field name="rule_group" ref="rule_group_sequence_strict"/>
        </record>

        <record model="res.user" id="user_trigger">
            <field name="login">user_cron_trigger</field>
            <field name="name">Cron Trigger</field>
            <field name="groups" eval="[('add', ref('group_admin'))]"/>
        </record>

        <record model="ir.cron" id="cron_trigger_time">
            <field name="name">Run On Time Triggers</field>
            <field name="request_user" ref="user_admin"/>
            <field name="user" ref="user_trigger"/>
            <field name="active" eval="True"/>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="model">ir.trigger</field>
            <field name="function">trigger_time</field>
        </record>

    </data>
</tryton>
