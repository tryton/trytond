                            <label name="model"/>
                            <field name="model"/>
                            <newline/>
                            <label name="email"/>
                            <field name="email" colspan="3"/>
                            <label name="window"/>
                            <field name="window"/>
                        </page>
                        <page name="keywords">
                            <field name="keywords" colspan="4">
                                <tree string="Keywords">
